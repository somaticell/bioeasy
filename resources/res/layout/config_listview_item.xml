<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:sweet="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
    <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:id="@+id/proValue" android:layout_width="match_parent" android:layout_height="wrap_content" android:minHeight="44dp">
        <TextView android:textSize="14sp" android:textColor="#888888" android:gravity="center" android:layout_gravity="center_vertical" android:id="@+id/tv_config_key" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="0.33"/>
        <View android:background="#dddddd" android:visibility="visible" android:layout_width="1px" android:layout_height="match_parent"/>
        <TextView android:textSize="14sp" android:textColor="#222222" android:gravity="center" android:id="@+id/tv_config_value" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="0.66"/>
    </LinearLayout>
    <View android:background="#dddddd" android:layout_width="match_parent" android:layout_height="1px"/>
</LinearLayout>
